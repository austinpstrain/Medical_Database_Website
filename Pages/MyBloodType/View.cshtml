@page
@model ViewModel
@using ClinicWeb.Model
@{
    ViewData["Title"] = "Blood Type Information";
}

<div class="card">
    <div class="card-body">
        <h3 class="card-title">@(ViewData["Title"])</h3>
        <table class="table">
            <thead>
                <tr>
                    <th scope="col">Blood Type</th>
                </tr>
            </thead>
            <tbody>
            @foreach (var type in Model.Type)
            {
                <tr>
                    <td>@(type.Name)</td>
                </tr>
            }
            </tbody>
        </table>
    </div>
</div>
